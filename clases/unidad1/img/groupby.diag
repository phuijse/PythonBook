blockdiag {
    #orientation = portrait
    default_fontsize = 14; 
    default_shape = roundedbox;
    A [label="split"]
    B [label="apply"]
    C [label="combine"]
    A -> B 
    B -> C 
    group {
          color = "#FFFF33";
          fontsize = 10;
          label = "groupby"
          A -> B;
          B -> C; 
      }

}
